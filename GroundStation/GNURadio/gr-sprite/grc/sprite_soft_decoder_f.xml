<?xml version="1.0"?>
<block>
  <name>Soft Decoder</name>
  <key>sprite_soft_decoder_f</key>
  <category>Sprite</category>
  <import>import sprite</import>
  <make>sprite.soft_decoder_f()</make>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <sink>
    <name>threshold</name>
    <type>float</type>
  </sink>

</block>
